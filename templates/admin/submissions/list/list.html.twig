{% extends '@Forumify/admin/base.html.twig' %}
{% block title_page %}
    {{ 'perscom.admin.submissions.list.title'|trans }}
{% endblock %}
{% block body %}
    {% if form %}
        <div class="breadcrumbs text-small">
            <a href="{{ path('perscom_admin_submission_list') }}">Submissions</a> &gt; {{ form.name }}
        </div>
    {% endif %}
    <h1>
        {{ 'perscom.admin.submissions.list.title'|trans }}
    </h1>
    {{ component('PerscomSubmissionTable', { form: form is not null ? form.id : null }) }}
{% endblock %}
