{
    "name": "forumify/forumify-perscom-plugin",
    "type": "forumify-plugin",
    "keywords": ["symfony-ux"],
    "license": "OSL-3.0",
    "require": {
        "php": ">=8.2",
        "forumify/forumify-platform": "^0.4.23",
        "deschutesdesigngroupllc/perscom-php-sdk": "^2.2.2",
        "league/commonmark": "^2.5",
        "symfony/lock": "^6.4|^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.13",
        "phpstan/phpstan-symfony": "^2.0",
        "slevomat/coding-standard": "^8.15"
    },
    "autoload": {
        "psr-4": {
            "Forumify\\PerscomPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "forumify-plugin-class": "Forumify\\PerscomPlugin\\ForumifyPerscomPlugin"
    }
}
