{
    "name": "forumify/forumify-perscom-plugin",
    "type": "forumify-plugin",
    "keywords": ["symfony-ux"],
    "license": "OSL-3.0",
    "require": {
        "php": ">=8.2",
        "forumify/forumify-platform": "^0.4.25",
        "deschutesdesigngroupllc/perscom-php-sdk": "3.0.0",
        "league/commonmark": "^2.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.13",
        "phpstan/phpstan-symfony": "^2.0",
        "slevomat/coding-standard": "^8.15"
    },
    "autoload": {
        "psr-4": {
            "Forumify\\PerscomPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "forumify-plugin-class": "Forumify\\PerscomPlugin\\ForumifyPerscomPlugin",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
